{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["views/*.pug", "public/**"]
      }
    }
  ],
  "scripts": {
    "build": "npm run deploy-auth0"
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    }
  ]
}
