{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["views/*.pug", "public/**"]
      }
    }
  ],
  "functions": {
    "npm run deploy-auth0": {
      "memory": 3008,
      "maxDuration": 240
    }
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    }
  ]
}
