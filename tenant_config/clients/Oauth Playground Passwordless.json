{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "Oauth Playground Passwordless",
  "is_first_party": true,
  "oidc_conformant": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "refresh_token": {
    "expiration_type": "expiring",
    "leeway": 0,
    "token_lifetime": 2592000,
    "idle_token_lifetime": 1296000,
    "infinite_token_lifetime": false,
    "infinite_idle_token_lifetime": false,
    "rotation_type": "rotating"
  },
  "allowed_clients": [],
  "callbacks": [
    "http://localhost:3001/authorization_code",
    "http://localhost:3001/authorization_code_pkce",
    "http://localhost:3001/implicit",
    "http://localhost:3001/password",
    "http://localhost:3001/client_credentials",
    "http://localhost:3001/refresh",
    "http://localhost:3001/device",
    "http://localhost:3001/passwordless",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/authorization_code_pkce",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/authorization_code",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/implicit",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/password",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/client_credentials",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/refresh",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/device",
    "https://oauthplayground-git-master-davidwestonauth0.vercel.app/passwordless"
  ],
  "native_social_login": {
    "apple": {
      "enabled": false
    },
    "facebook": {
      "enabled": false
    }
  },
  "custom_login_page": "./Oauth Playground Passwordless_custom_login_page.html",
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "none",
  "app_type": "spa",
  "grant_types": [
    "authorization_code",
    "implicit",
    "refresh_token"
  ],
  "custom_login_page_on": true
}
